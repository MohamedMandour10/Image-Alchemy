    margin: 0px;
    border: 1px solid #283999;
	border-top-left-radius: 18px;
	border-top-right-radius: 18px; /* Border radius for top-right corner */
}

QTabBar::tab:!selected {
    margin-top: 0px; /* make non-selected tabs look smaller */
}
